<script>
	 me.require(['viewer&css'], 'bundle');
  me.require.ready('bundle',function(){
    var pictures = document.querySelector('.docs-pictures');
  var options = {
    // inline: true,
    url: 'data-original',
    inline: true,
    ready:  function (e) {
