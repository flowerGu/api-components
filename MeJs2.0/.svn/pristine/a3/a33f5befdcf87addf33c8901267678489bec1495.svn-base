*{
    margin:0;
    padding:0;
    
}
.box-sizing{
    box-sizing:border-box;
    transition:all .3s ;
}
input{
    border:none;
    outline: none;
    cursor:pointer;
    border:none;
}
.me-cascade-picker-wrap{
    position:relative;
    display:inline-block;
    width:100%;
    height:32px;
    line-height:100%;
    border-radius:4px;
    font-size:12px;
    cursor:pointer;
    border-radius:4px;
    border:1px solid #d9d9d9;
    color: rgba(0,0,0,.65); 
    user-select: none;    
    -webkit-user-select:none;
    -moz-user-select: none;
}
.me-cascade-picker-wrap input[readonly],.me-cascade-picker-wrap input[type="text"]{
    cursor: pointer;
    height:100%;
    border:none;
}
.me-cascade-picker-wrap.large{
    height:30px;
    line-height:30px;
    width:400px;
}
.me-cascade-picker-input,.me-cascade-picker-content{
    position:absolute;
    z-index:2;
    width:100%;
    height:100%;
    line-height:100%;
    padding:10px;
    background-color:transparent!important;
}
span.me-cascade-picker-content{
    display:block;
    width:85%;
    z-index:1;
    border:none;
    text-overflow:ellipsis;
    white-space: nowrap;
    overflow:hidden;
}
input.me-cascade-picker-input.active{
     border-color: #49a9ee;
     box-shadow: 0 0 0 2px rgba(16,142,233,.2);
}
span.me-cascade-picker-span{
    position:absolute;
    left:0;
    top:0;
    z-index:1;
    width:100%;
    height:100%;
}
i.me-cascade-arrow,i.me-cascade-cross{
    position:absolute;
    right:5%;
    top:50%;
    margin-top:-4px;
    z-index:1;
    display:inline-block;
    width:8px;
    height:8px;
    line-height:8px;
    color:#919191 !important;
}
i.me-cascade-cross{    
    z-index:4;
}
i.me-cascade-arrow.rotate{
    transform: rotate(180deg);
}
.me-cascade-list-wrap{
    position:absolute;
    height:180px;    
    z-index: 99999;
    border-radius: 4px;
    font-size:12px;
    white-space: nowrap;
    color:rgba(0,0,0,.65);
}
.me-cascade-list-wrap:after{
    clear:both;
    content:' ';
    height:0;
    line-height:0;
    display:block;
    overflow:hidden;
}
.me-cascade-list-wrap ul{
    overflow: auto;
    max-height:180px;
    height:180px;
    list-style: none;   
    display: inline-block;
    border-right: 1px solid #e9e9e9;
    background-color:#fff;
    box-shadow: 0 1px 6px rgba(0,0,0,.2);
}
.me-cascade-list-wrap ul:last-child{
    border:none;
}
.me-hide{
     display:none;
}
.me-show{
    display:block;
}
.me-cascade-picker-wrap *.me-hide{
    display:none;
}
.me-cascade-picker-wrap *.me-show{
    display:block;
}
.me-cascader-menu-item {
    position:relative;
    padding: 5px 26px 5px 16px;
    cursor: pointer;
    white-space: nowrap;
    border-radius: 4px;
    -webkit-transition: all .3s ease;
    transition: all .3s ease
}
.me-cascader-menu-item[disabled]{
    cursor:not-allowed}

.me-cascader-menu-item[disabled]:hover{
    background:transparent;
}
.me-cascader-menu-item-active{
    background-color: #f7f7f7;
    font-weight: 700;
}
.me-cascader-menu-item:hover {
    background: #ecf6fd
}

.me-cascader-menu-item.me-ion-o-arrow-right:before,.me-cascader-menu-item.me-ion-load-c:before{
    position:absolute;
    right:5%;
    top:50%;
    transform: translateY(-50%)
}
.me-cascader-menu-item.me-ion-load-c:before{
    top:35%;
    -webkit-animation: loadingCircle 1s infinite linear;
    animation: loadingCircle 1s infinite linear
}
.me-cascade-list:last-child .me-ion-o-arrow-right:before,.me-cascade-list:last-child .me-ion-load-c:before{
    content:''
}
/*向下展开*/
.fadeInDown{
    -webkit-animation:fadeInDown .5s .2s ease both;
    -moz-animation:fadeInDown .5s .2s ease both    
}
@-webkit-keyframes fadeInDown{
    0%{opacity:0;
    -webkit-transform:translateY(-10px)}
    100%{opacity:1;
    -webkit-transform:translateY(0)}
}
@-moz-keyframes fadeInDown{
    0%{opacity:0;
    -moz-transform:translateY(-10px)}
    100%{opacity:1;
    -moz-transform:translateY(0)}
}


/*向上展开*/
.fadeINUp{
    -webkit-animation:fadeInUp .5s .2s ease both;
    -moz-animation:fadeInUp .5s .2s ease both;
}
@-webkit-keyframes fadeInUp{
    0%{
        opacity:0;
        -webkit-transform:translateY(10px)
    }
    100%{
        opacity:1;
        -webkit-transform:translateY(0)
    }
}
@-moz-keyframes fadeInUp{
    0%{
        opacity:0;
        -moz-transform:translateY(10px)
    }
    100%{
        opacity:1;
        -moz-transform:translateY(0)
    }
}
@-webkit-keyframes loadingCircle {
    0% {
        -webkit-transform-origin: 50% 50%;
        transform-origin: 50% 50%;
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    to {
        -webkit-transform-origin: 50% 50%;
        transform-origin: 50% 50%;
        -webkit-transform: rotate(1turn);
        transform: rotate(1turn)
    }
}

@keyframes loadingCircle {
    0% {
        -webkit-transform-origin: 50% 50%;
        transform-origin: 50% 50%;
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg)
    }

    to {
        -webkit-transform-origin: 50% 50%;
        transform-origin: 50% 50%;
        -webkit-transform: rotate(1turn);
        transform: rotate(1turn)
    }
}