<script>
  me.require(['menu&css'], 'menu');
  me.require.ready('menu',function(){
    me.menu.render({
      ele: 'vertical',
      mode: 'vertical',
      selectedHide: false,
      callback: function(ele){
        console.log(ele)
      }
    });
    me.menu.render({
      ele: 'inline',
      mode: 'inline',
      selectedHide: false,
      callback: function(ele){
        console.log(ele)
      }
    });
    me.menu.render({
      ele: 'horizontal',
      mode: 'horizontal',
      selectedHide: false,
      callback: function(ele){
        console.log(ele)
      }
    });
  });
</script>
